{
	"type": "not-things_timeseq_script",
	"version": "0.0.1",
	"global-actions": [
		{
			"timing": "start",
			"set-polyphony": { "index": 1, "channels": 2 }
		},
		{
			"timing": "start",
			"set-polyphony": { "index": 2, "channels": 2 }
		},
		{
			"timing": "start",
			"set-polyphony": { "index": 3, "channels": 3 }
		},
		{
			"timing": "start",
			"set-value": {
				"output": { "index": 1 },
				"value": { "voltage": 1.01 }
			}
		},
		{
			"timing": "start",
			"set-value": {
				"output": { "index": 2 },
				"value": { "note": "E3+" }
			}
		},
		{
			"timing": "end",
			"set-value": {
				"output": { "index": 1 },
				"value": { "voltage": -1.01 }
			}
		},
		{
			"timing": "start",
			"set-value": {
				"output": { "index": 2 },
				"value": { "note": "F5-" }
			}
		}
	],
	"timelines": [
		{
			"lanes": [
				{
					"auto-start": true,
					"loop": true,
					"segments": [
						{
							"duration": { "hz": 130.815 },
							"actions": [
								{
									"timing": "glide",
									"start-value": { "voltage": -5.0 },
									"end-value": { "voltage": 5.0 },
									"output": { "index": 7 }
								}
							]
						}
					]
				},
				{
					"auto-start": false,
					"loop": false,
					"start-trigger": "do-it",
					"stop-trigger": "stop-it",
					"segments": [
						{
							"duration": { "samples": 1 },
							"actions": [
								{
									"timing": "start",
									"set-value": {
										"value": {
											"rand": {
												"lower": { "input": { "index": 1 } },
												"upper": { "input": { "index": 2 } }
											}
										},
										"output": { "index": 6 }
									}
								}
							]
						}
					]
				},
				{
					"auto-start": false,
					"loop": false,
					"start-trigger": "bernoulli",
					"segments": [
						{
							"duration": { "millis": 500 },
							"actions": [
								{
									"timing": "start",
									"set-variable": {
										"name": "bernoulli-gate",
										"value": {
											"rand": {
												"lower": { "voltage": -1.0 },
												"upper": { "voltage": 1.0 }
											}
										}
									}
								},
								{
									"timing": "start",
									"if": {
										"lt": [
											{ "variable": "bernoulli-gate" },
											{ "voltage": 0.0 }
										]
									},
									"set-value": {
										"output": { "index": 3, "channel": 1 },
										"value": { "voltage": 10 }
									}
								},
								{
									"timing": "start",
									"if": {
										"lt": [
											{ "variable": "bernoulli-gate" },
											{ "voltage": 0.0 }
										]
									},
									"set-value": {
										"output": { "index": 3, "channel": 2 },
										"value": { "voltage": 0 }
									}
								},
								{
									"timing": "start",
									"if": {
										"gte": [
											{ "variable": "bernoulli-gate" },
											{ "voltage": 0.0 }
										]
									},
									"set-value": {
										"output": { "index": 3, "channel": 1 },
										"value": { "voltage": 0 }
									}
								},
								{
									"timing": "start",
									"if": {
										"gte": [
											{ "variable": "bernoulli-gate" },
											{ "voltage": 0.0 }
										]
									},
									"set-value": {
										"output": { "index": 3, "channel": 2 },
										"value": { "voltage": 10 }
									}
								},
								{
									"timing": "start",
									"set-value": {
										"output": { "index": 3, "channel": 3 },
										"value": { "variable": "bernoulli-gate" }
									}
								}
							]
						},
						{
							"duration": { "millis": 500 },
							"actions": [
								{
									"timing": "start",
									"set-value": {
										"output": { "index": 3, "channel": 1 },
										"value": { "voltage": 0 }
									}
								},
								{
									"timing": "start",
									"set-value": {
										"output": { "index": 3, "channel": 2 },
										"value": { "voltage": 0 }
									}
								}
							]
						}
					]
				}
			]
		}
	],
	"input-triggers": [
		{
			"id": "do-it",
			"input": { "index": 8 }
		},
		{
			"id": "stop-it",
			"input": { "index": 7 }
		},
		{
			"id": "bernoulli",
			"input": { "index": 6 }
		}
	]
}
