{
	"type": "not-things_timeseq_script",
	"version": "0.0.1",
	"timelines": [
		{
			"lanes": [
				{
					"auto-start": false,
					"loop": false,
					"start-trigger": "determine-output",
					"segments": [
						{
							"duration": { "millis": 1 },
							"actions": [
								{
									"set-variable": {
										"name": "bernoulli-chance",
										"value": {
											"rand": {
												"lower": { "voltage": 0 },
												"upper": { "voltage": 10 }
											}
										}
									}
								},
								{
									"set-value": {
										"output": { "index": 3 },
										"value": { "variable": "bernoulli-chance" }
									}
								},
								{
									"if": {
										"gt": [
											{ "variable": "bernoulli-chance" },
											{ "input": { "index": 2 } }
										]
									},
									"set-value": {
										"output": { "index": 1 },
										"value": { "voltage": 10 }
									}
								},
								{
									"if": {
										"lte": [
											{ "variable": "bernoulli-chance" },
											{ "input": { "index": 2 } }
										]
									},
									"set-value": {
										"output": { "index": 2 },
										"value": { "voltage": 10 }
									}
								},
								{
									"timing": "end",
									"trigger": "reset-output"
								}
							]
						}
					]
				},
				{
					"auto-start": false,
					"loop": true,
					"start-trigger": "reset-output",
					"stop-trigger": "stop-reset-output",
					"segments": [
						{
							"duration": { "samples": 1 },
							"actions": [
								{
									"if": {
										"lt": [
											{ "input": { "index": 1 } },
											{ "voltage": 1 }
										]
									},
									"set-value": {
										"output": { "index": 1 },
										"value": { "voltage": 0 }
									}
								},
								{
									"if": {
										"lt": [
											{ "input": { "index": 1 } },
											{ "voltage": 1 }
										]
									},
									"set-value": {
										"output": { "index": 2 },
										"value": { "voltage": 0 }
									}
								},
								{
									"if": {
										"lt": [
											{ "input": { "index": 1 } },
											{ "voltage": 1 }
										]
									},
									"trigger": "stop-reset-output"
								}
							]
						}
					]
				}
			]
		}
	],
	"input-triggers": [
		{
			"id": "determine-output",
			"input": { "index": 1 }
		}
	]
}
