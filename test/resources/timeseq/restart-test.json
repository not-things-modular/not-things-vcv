							]
						}
					]
				},
				{
					"auto-start": true,
					"loop": true,
					"segments": [
						{
							"duration": { "millis": 500 },
							"actions": [
								{
									"timing": "start",
									"set-value": {
										"output": { "index": 1 },
										"value": { "variable": "the-variable" }
